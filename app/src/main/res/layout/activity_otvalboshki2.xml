<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Otvalboshki2"
    android:background="@drawable/component_24">


  <LinearLayout
      android:layout_width="328dp"
      android:layout_height="463dp"
      android:orientation="vertical"
      app:layout_constraintBottom_toBottomOf="parent"
      app:layout_constraintEnd_toEndOf="parent"
      app:layout_constraintHorizontal_bias="0.493"
      app:layout_constraintStart_toStartOf="parent"
      app:layout_constraintTop_toTopOf="parent"
      app:layout_constraintVertical_bias="0.984">


    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/name"
        android:textColor="@color/white"
        android:textSize="17sp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="267dp" />

    <androidx.cardview.widget.CardView
        android:layout_width="320dp"
        android:layout_height="40dp"
        android:layout_marginBottom="10dp"
        app:cardCornerRadius="100dp">

      <EditText
          android:id="@+id/textView7"
          android:layout_width="320dp"
          android:layout_height="40dp"
          android:autofillHints="username"
          android:background="@color/teal_700"
          android:inputType="textPersonName"
          android:text="@string/stroke1"
          android:textColor="@color/white"
          tools:ignore="LabelFor" />
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Surname"
        android:textColor="@color/white"
        android:textSize="17sp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="343dp" />

    <androidx.cardview.widget.CardView
        android:layout_width="320dp"
        android:layout_height="40dp"
        android:layout_marginBottom="10dp"
        app:cardCornerRadius="100dp">

      <EditText
          android:id="@+id/textView8"
          android:layout_width="320dp"
          android:layout_height="40dp"
          android:autofillHints="username2"
          android:background="@color/teal_700"
          android:inputType="textPassword"
          android:text="@string/stroke2"
          android:textColor="@color/white"
          tools:ignore="LabelFor" />
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/gmail"
        android:textColor="@color/white"
        android:textSize="17sp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="419dp" />

    <androidx.cardview.widget.CardView
        android:layout_width="320dp"
        android:layout_height="40dp"
        android:layout_marginBottom="10dp"
        app:cardCornerRadius="100dp">

      <EditText
          android:id="@+id/textView9"
          android:layout_width="320dp"
          android:layout_height="40dp"
          android:autofillHints="emailAddress"
          android:background="@color/teal_700"
          android:inputType="textEmailAddress"
          android:text="@string/stroke3"
          android:textColor="@color/white"
          tools:ignore="LabelFor" />
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/number"
        android:textColor="@color/white"
        android:textSize="17sp"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="496dp" />

    <androidx.cardview.widget.CardView
        android:layout_width="320dp"
        android:layout_height="40dp"
        android:layout_marginBottom="20dp"
        app:cardCornerRadius="100dp">

      <EditText
          android:id="@+id/textView10"
          android:layout_width="320dp"
          android:layout_height="40dp"
          android:autofillHints="phone"
          android:background="@color/teal_700"
          android:inputType="textPhonetic"
          android:text="@string/stroke4"
          android:textColor="@color/white"
          tools:ignore="LabelFor"
          tools:layout_editor_absoluteX="45dp"
          tools:layout_editor_absoluteY="519dp" />
    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="326dp"
        android:layout_height="62dp"
        android:layout_marginBottom="10dp"
        app:cardCornerRadius="100dp">

      <Button
          android:id="@+id/button3"
          android:layout_width="326dp"
          android:layout_height="62dp"
          android:background="#FEF85F"
          android:text="@string/next2"
          android:textColor="@color/black"
          android:textSize="25sp"
          android:textStyle="bold"
          tools:layout_editor_absoluteX="41dp"
          tools:layout_editor_absoluteY="583dp" />
    </androidx.cardview.widget.CardView>


    <TextView
        android:id="@+id/textView11"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:text="@string/account"
        android:textColor="@color/white" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:text="@string/welcome2"
        android:textColor="@color/yellow"
        android:textSize="17sp" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center"
        android:text="@string/next3"
        android:textColor="@color/white" />

  </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>